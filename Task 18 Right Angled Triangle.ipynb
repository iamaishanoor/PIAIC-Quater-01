   "source": [
    "import math\n",
    "a = float(input(\"Enter the value of 'a': \"))\n",
    "b = float(input(\"Enter the value of 'b': \"))\n",
    "c = math.sqrt(pow(a,2) + pow(b,2))\n",
    "c = round(c,2)\n",
    "print(\"Hypotenuse of a Right Angled Triangle is: \"+str(c))"
